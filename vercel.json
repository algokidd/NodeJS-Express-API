{
    "version": 2,
    "name": "node-js-express-api",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "scripts": {
                "test": "echo \"Error: no test specified\" && exit 1",
                "start": "nodemon server.js"
            },
            "src": "/(.*)",
            "use": "@vercel/node",
            "methods": [
                "GET",
                "PUT",
                "POST",
                "PATCH",
                "DELETE",
                "OPTIONS",
                "HEAD"
            ],
            "dest": "/server.js"
        }
    ]
}