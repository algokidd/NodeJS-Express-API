{
    "version": 2,
    "name": "node-js-express-api",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": [
                "GET",
                "PUT",
                "POST",
                "PATCH",
                "DELETE",
                "OPTIONS",
                "HEAD"
            ],
            "dest": "/server.js"
        }
    ]
}